pipeline {

	agent any 
	
	stages {
		stage('Compile Stage'){
			steps{
				
					sh 'mvn clean compile'
				
			}
		}
		stage('Testing Stage'){
			steps{
				
					sh 'mvn clean test --define xmlfileName={testXmlFile}'
					cucumber fileIncludePattern: '**/target/cucumber-reports/*.json'
			}
		}
	}
}
